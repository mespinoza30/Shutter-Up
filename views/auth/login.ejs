<form action="/auth/login" method="POST">
  <div id="divider"></div>
  <br>
  <h2 class="text">Please Login to continue</h2>
  <div id="profImage">
    <img src="https://res.cloudinary.com/dvzw25pxu/image/upload/v1613954506/1camlense_h5fzqf.png" alt="Shutter Image">
  </div>
  <br>
  <div class="text">
    <label for="auth-email">Email</label>
    <input id="auth-email" type="email" name="email">

    <label for="auth-password">Password</label>
    <input id="auth-password" type="password" name="password">

    <input type="submit" value="Log In">
</div>
</form>
